<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- CAS验证服务器地址 -->
	<loginServer>https://sso.scut.edu.cn/cas</loginServer>
	<validateServer>https://sso.scut.edu.cn/cas</validateServer>
	<!--平台地址-->
	<serverName>http://127.0.0.1:8080</serverName>
	<!-- 排除被保护的资源  -->
	<notForceAuthUrls>
		<url-pattern>\.js$</url-pattern>
		<url-pattern>\.xml$</url-pattern>
		<url-pattern>\.html.*</url-pattern>
		<url-pattern><![CDATA[.*/fileUpload.*]]></url-pattern>
	</notForceAuthUrls>
</config>